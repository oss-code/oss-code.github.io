<dec f='include/llvm-6.0/llvm/IR/Constants.h' l='285' type='static llvm::Constant * llvm::ConstantFP::get(llvm::Type * Ty, double V)'/>
<doc f='include/llvm-6.0/llvm/IR/Constants.h' l='281'>/// This returns a ConstantFP, or a vector containing a splat of a ConstantFP,
  /// for the specified value in the specified type. This should only be used
  /// for simple constant values like 2.0/1.0 etc, that are known-valid both as
  /// host double and as the target format.</doc>
