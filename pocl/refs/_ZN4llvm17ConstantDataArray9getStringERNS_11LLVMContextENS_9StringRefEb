<dec f='include/llvm-6.0/llvm/IR/Constants.h' l='714' type='static llvm::Constant * llvm::ConstantDataArray::getString(llvm::LLVMContext &amp; Context, llvm::StringRef Initializer, bool AddNull = true)'/>
<doc f='include/llvm-6.0/llvm/IR/Constants.h' l='709'>/// This method constructs a CDS and initializes it with a text string.
  /// The default behavior (AddNull==true) causes a null terminator to
  /// be placed at the end of the array (increasing the length of the string by
  /// one more than the StringRef would normally indicate.  Pass AddNull=false
  /// to disable this behavior.</doc>
