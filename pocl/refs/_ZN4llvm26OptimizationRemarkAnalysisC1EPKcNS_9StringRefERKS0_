<def f='include/llvm-6.0/llvm/IR/DiagnosticInfo.h' l='762' ll='764' type='void llvm::OptimizationRemarkAnalysis::OptimizationRemarkAnalysis(const char * PassName, llvm::StringRef Prepend, const llvm::OptimizationRemarkAnalysis &amp; Orig)'/>
<doc f='include/llvm-6.0/llvm/IR/DiagnosticInfo.h' l='755'>/// \brief This is ctor variant allows a pass to build an optimization remark
  /// from an existing remark.
  ///
  /// This is useful when a transformation pass (e.g LV) wants to emit a remark
  /// (\p Orig) generated by one of its analyses (e.g. LAA) as its own analysis
  /// remark.  The string \p Prepend will be emitted before the original
  /// message.</doc>
